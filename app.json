{
  "expo": {
    "name": "全年营销日历",
    "slug": "marketing-calendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": ".",
      "favicon": "./yingxiaorili/favicon.png",
      "publicPath": "./yingxiaorili/", 
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons",
            "react-native-vector-icons"
          ]
        }
      }
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "./yingxiaorili/node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/*.ttf"
    ],
    "fonts": [
      "./yingxiaorili/assets/fonts/SpaceMono-Regular.ttf",
      "./yingxiaorili/node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/FontAwesome.ttf"
    ],
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
